            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">String header = vars.get(&quot;APP_HEADER&quot;);
String[] _header = header.split(&quot;=&quot;);
String header_key = _header[0];
String header_value = _header[_header.length - 1];

String cookie = vars.get(&quot;APP_COOKIE&quot;);
String[] _cookie = cookie.split(&quot;=&quot;);
String cookie_key = _cookie[0];
String cookie_value = _cookie[_cookie.length - 1];

vars.put(&quot;header_key&quot;, header_key);
vars.put(&quot;header_value&quot;, header_value);
vars.put(&quot;cookie_key&quot;, cookie_key);
vars.put(&quot;cookie_value&quot;, cookie_value);</stringProp>
          </JSR223Sampler>
